{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
        "@/*": [
            "./src/*"
        ],
        "@config": [
            "src/server/config/index.ts"
        ],
        "@config/*": [
            "src/server/config/*"
        ],
        "@controllers/*": [
            "./src/server/controllers/*"
        ],
        "@components/*": [
            "./src/server/views/pages/components/*"
        ],
        "@infra/*": [
            "./src/server/infra/*"
        ],
        "@lib/*": [
            "src/lib/*"
        ],
        "@models/*": [
            "./src/server/models/*"
        ],
        "@modules/*": [
            "./src/server/modules/*"
        ],
        "@node_modules/*": [
            "node_modules/*"
        ],
        "@schedulers/*": [
            "./src/server/schedulers/*"
        ],
        "@services/*": [
            "./src/server/services/*"
        ],
        "@views": [
            "./src/server/views/index.ts"
        ],
        "@views/*": [
            "./src/server/views/*"
        ],
        "@db": [
            "./src/server/db/index.ts"
        ],
        "@db/*": [
            "./src/server/db/*"
        ],
        "@betagouv": [
            "./src/server/betagouv.ts"
        ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"],
    "transpileOnly": true,
    "compilerOptions": {
      "module": "commonjs",
        "target": "es6",
        "paths": {
            "@config": [
                "./src/config/index.ts"
            ],
            "@config/*": [
                "./src/config/*"
            ],
            "@controllers/*": [
                "./src/server/controllers/*"
            ],
            "@components/*": [
                "./src/server/views/pages/components/*"
            ],
            "@infra/*": [
                "./src/server/infra/*"
            ],
            "@lib/*": [
                "./src/lib/*"
            ],
            "@models/*": [
                "./src/server/models/*"
            ],
            "@modules/*": [
                "./src/server/modules/*"
            ],
            "@node_modules/*": [
                "node_modules/*"
            ],
            "@schedulers/*": [
                "./src/server/schedulers/*"
            ],
            "@services/*": [
                "./src/server/services/*"
            ],
            "@views": [
                "./src/server/views/index.ts"
            ],
            "@views/*": [
                "./src/server/views/*"
            ]
        },
        "plugins": [
            {
                "transform": "typescript-transform-paths"
            }
            ]
    }
  },
  "ts-mocha": {
    "require": ["tsconfig-paths/register"],
    "transpileOnly": true,
    "compilerOptions": {
      "module": "commonjs",
        "target": "es6",
        "paths": {
            "@config": [
                "./src/config/index.ts"
            ],
            "@config/*": [
                "./src/config/*"
            ],
            "@controllers/*": [
                "./src/server/controllers/*"
            ],
            "@components/*": [
                "./src/server/views/pages/components/*"
            ],
            "@infra/*": [
                "./src/server/infra/*"
            ],
            "@lib/*": [
                "./src/lib/*"
            ],
            "@models/*": [
                "./src/server/models/*"
            ],
            "@modules/*": [
                "./src/server/modules/*"
            ],
            "@node_modules/*": [
                "node_modules/*"
            ],
            "@schedulers/*": [
                "./src/server/schedulers/*"
            ],
            "@services/*": [
                "./src/server/services/*"
            ],
            "@views": [
                "./src/server/views/index.ts"
            ],
            "@views/*": [
                "./src/server/views/*"
            ]
        },"plugins": [
            {
                "transform": "typescript-transform-paths"
            }
            ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "types.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
