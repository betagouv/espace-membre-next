# Environment
NODE_ENV=development
TZ=UTC

# Application
SESSION_SECRET=local-dev-secret-123
HASH_SALT=development-salt-123
SECURE=false
HOSTNAME=localhost

# Database
DATABASE_URL=postgres://secretariat:secretariat@localhost:5432/secretariat
NEXTAUTH_DATABASE_URL=postgres://secretariat:secretariat@localhost:5432/nextauth
SEED_FAKE_DATA_DATABASE=true

# Redis
REDIS_URL=redis://:localpwd@localhost:6379

# Mail
MAIL_HOST=localhost
MAIL_PORT=1025
MAIL_IGNORE_TLS=true
MAIL_USER=
MAIL_PASS=
MAIL_SENDER=espace-membre@incubateur.net

# Chat Webhooks
CHAT_WEBHOOK_URL_SECRETARIAT=http://localhost:8100/api/chat/webhook/secretariat
CHAT_WEBHOOK_URL_GENERAL=http://localhost:8100/api/chat/webhook/general
CHAT_WEBHOOK_URL_DINUM=http://localhost:8100/api/chat/webhook/dinum
CHAT_WEBHOOK_URL_GIP=http://localhost:8100/api/chat/webhook/gip

# OVH
OVH_APP_KEY=dev-app-key
OVH_APP_SECRET=dev-app-secret
OVH_CONSUMER_KEY=dev-consumer-key

# GitHub
GITHUB_TOKEN=your-github-token
GITHUB_REPOSITORY=betagouv/beta.gouv.fr
GITHUB_FORK=your-username/beta.gouv.fr
GITHUB_ORG_ADMIN_TOKEN=your-github-admin-token

# Mailing Lists
MAILING_LIST_NEWSLETTER=newsletter@beta.gouv.fr
NEWSLETTER_BROADCAST_LIST=secretariat@beta.gouv.fr

# Optional Features
FEATURE_MATTERMOST_REMOVE_USERS=false
FEATURE_SIB_USE_UPDATE_CONTACT_EMAIL=false
FEATURE_USE_NEW_MARRAINAGE=true

# API URLs (optional)
SPONSOR_API=https://beta.gouv.fr/api/v2.6/sponsors.json
USERS_API=https://beta.gouv.fr/api/v2.6/authors.json
INCUBATOR_API=https://beta.gouv.fr/api/v2.6/incubators.json
STARTUPS_API=https://beta.gouv.fr/api/v2.6/startups.json
STARTUPS_DETAILS_API=https://beta.gouv.fr/api/v2.6/startups_details.json
